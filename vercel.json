{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/scheduled-robots",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "src/app/api/inngest/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/executions/stream/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/worker/callback/route.ts": {
      "maxDuration": 60
    }
  }
}
